#include <stdio.h>
#include <stdlib.h>
int main() {
    char password[100];
    FILE *fp;
    fp = fopen("101-password", "w");
    if (fp == NULL) {
        printf("Error opening file\n");
        return 1;
    }
    printf("Enter password: ");
    scanf("%s", password);
    fprintf(fp, "%s", password);
    fclose(fp);
    return 0;
}

